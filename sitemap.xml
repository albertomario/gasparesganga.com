---
<?xml version="1.0" encoding="utf-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
	{% include vars.html %}
    <url>
		<loc>{{ vars.site_url }}</loc>
		<priority>1.0</priority>
	</url>
	<url>
		<loc>{{ vars.site_url }}/labs/</loc>
		<priority>1.0</priority>
	</url>
	<url>
		<loc>{{ vars.site_url }}/posts/</loc>
		<priority>1.0</priority>
	</url>
	{% for lab in site.pages %}
		{% if lab.layout == "lab" %}
			<url>
				<loc>{{ vars.site_url }}{{ lab.url | replace: '/index.html', '' }}</loc>
				<lastmod>{{ lab.updated | date: "%Y-%m-%d" }}</lastmod>
				<priority>1.0</priority>
			</url>
		{% endif %}
	{% endfor %}
	{% for post in site.posts %}
		<url>
			<loc>{{ vars.site_url }}{{ post.url }}</loc>
			<lastmod>{{ post.date | date: "%Y-%m-%d" }}</lastmod>
			<priority>1.0</priority>
		</url>
	{% endfor %}
    <url>
		<loc>{{ vars.site_url }}/coffee/</loc>
		<priority>1.0</priority>
	</url>
</urlset>
